<!-- previous-orders handlebars-->

<tbody class="previous-orders-table">
  {{#each orders as |order|}}
  <tr>
    <th width="250px">Order #: {{order.id}}</th>
  </tr>
</tbody>
<tbody class="previous-orders-table">
  <tr>
    <th>Name</th>
    <th width="250px">Category</th>
    <th>Price</th>
  </tr>
  {{#each order.products as |product|}}
  <tr>
    <td>{{product.name}}</td>
    <td>{{product.category}}</td>
    <td>{{product.price}}</td>
  </tr>
  {{/each}}
</tbody>
  {{/each}}
